{
  "name": "w8mngr-2020",
  "version": "0.0.1",
  "description": "w8mngr utilizing Netlify's functions and a React SPA",
  "scripts": {
    "test": "NODE_ENV=test mocha -r ts-node/register -r tsconfig-paths/register --recursive --exit --watch-extensions ts",
    "test:all": "npm run test -- api/**/*.test.[jt]s shared/**/*.test.[jt]s client/**/*.test.[jt]s",
    "test:api": "npm run test -- api/**/*.test.[jt]s",
    "test:shared": "npm run test -- shared/**/*.test.[jt]s",
    "test:client": "npm run test -- client/**/*.test.[jt]s",
    "test:init": "psql -f test/initial_structure.sql",
    "start:lambda": "ts-node-dev --project tsconfig.api.json --respawn --transpileOnly ./serve.ts",
    "build:lambda": "tsc --project tsconfig.api.json",
    "build:client": "webpack --config webpack.config.js",
    "start:client": "WATCH=1 NODE_ENV=development webpack-dev-server --hot"
  },
  "_moduleAliases": {
    "api": "./api",
    "shared": "./shared",
    "client": "./client"
  },
  "author": "Ryan M. Poe",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@momothepug/tsmodule-alias": "^1.0.0",
