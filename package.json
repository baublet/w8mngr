{
  "name": "w8mngr",
  "version": "0.0.2",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "description": "w8mngr - the lifestyle tracker",
  "scripts": {
    "build:api:command": "esbuild --sourcemap --bundle --platform=node --external:knex --external:bcrypt --external:@netlify/functions",
    "build:api:structure": "esbuild api/**/*.ts api/*.ts --outdir=tmp",
    "build:api:gql": "pnpm build:api:command api/graphql.ts --outfile=api/graphql.js",
    "build:api:worker": "pnpm build:api:command api/worker.ts --outfile=api/worker.js",
    "build:api": "pnpm build:api:gql && pnpm build:api:worker",
    "build:client": "pnpm vite build",
    "check": "pnpm tsc --noEmit",
    "gql:generate": "graphql-codegen",
    "postinstall": "husky install && pnpm gql:generate",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "db:delete": "rm -rf tmp/v3/d1",
    "db:create:migration": "pnpm wrangler d1 migrations create w8mngr_1 -c graphql.wrangler.toml",
    "db:generateTypes": "DATABASE_URL=$(find ./tmp/v3/d1 -type f -name '*.sqlite' -print -quit) pnpm kysely-codegen --out-file api/config/db.types.ts; echo \"// AUTOMATICALLY GENERATED FILE. GENERATE WITH: pnpm db:generateTypes\n\n$(cat api/config/db.types.ts)\" > api/config/db.types.ts",
    "db:migrate": "pnpm db:migrate:command --local --persist-to tmp && pnpm db:generateTypes",
    "db:migrate:command": "pnpm wrangler d1 migrations apply w8mngr_1 -c graphql.wrangler.toml",
    "db:reset": "pnpm db:delete; pnpm db:migrate",
    "develop:api": "wrangler dev -c graphql.wrangler.toml api/graphql --persist-to tmp --port 8787 --var DANGEROUSLY_LOG_EMAIL_LOGIN_LINKS:true SUPPRESS_CONSOLE_LOGGING:false",
    "develop:ui": "pnpm vite",
    "develop": "concurrently --kill-others npm:develop:api npm:develop:ui",
    "lint:tsc": "pnpm lint:tsc:api && pnpm lint:tsc:client && pnpm lint:tsc:shared",
    "lint:tsc:api": "pnpm tsc -p api/tsconfig.json",
    "lint:tsc:client": "pnpm tsc -p client/tsconfig.json",
    "lint:tsc:shared": "pnpm tsc -p shared/tsconfig.json",
    "prettier:all": "prettier --write \"**/*.{js,ts,tsx,json,md,graphql,css,scss,html}\"",
    "seed:exercises": "./scripts/createActivityLibrarySql/seed.sh"
  },
  "author": "Ryan M. Poe",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@apollo/server": "^4.7.4",
    "@as-integrations/cloudflare-workers": "^0.1.1",
    "@baublet/service-container": "^0.8.3",
    "@cloudflare/workers-types": "^4.20230518.0",
    "@ericblade/quagga2": "^1.8.2",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/import-types-preset": "^2.2.6",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-codegen/typescript-resolvers": "^4.0.0",
    "@popperjs/core": "^2.11.2",
    "@swc/core": "^1.3.64",
    "@swc/helpers": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/js-quantities": "^1.6.3",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/lodash.groupby": "^4.6.7",
    "@types/lodash.omit": "^4.5.7",
    "@types/object-hash": "^3.0.2",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@vitejs/plugin-react": "^4.0.0",
    "add": "^2.0.6",
    "axios": "^1.4.0",
    "better-sqlite3": "^8.4.0",
    "classnames": "^2.3.2",
    "concurrently": "^8.2.0",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "date-fns": "^2.30.0",
    "esbuild": "^0.18.13",
    "esbuild-register": "^3.4.2",
    "form-data": "^4.0.0",
    "graphql": "^16.7.1",
    "husky": ">=6",
    "js-quantities": "^1.7.6",
    "json-stringify-deterministic": "^1.0.8",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^9.0.0",
    "kysely": "^0.27.1",
    "kysely-codegen": "^0.11.0",
    "kysely-d1": "^0.3.0",
    "lint-staged": ">=10",
    "lodash.clamp": "^4.0.3",
    "lodash.groupby": "^4.6.0",
    "lodash.keyby": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lru-cache": "^10.0.0",
    "minifaker": "^1.34.1",
    "object-hash": "^3.0.0",
    "pinst": ">=2",
    "prettier": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-popper": "^2.3.0",
    "recharts": "^2.7.1",
    "typescript": "^5.1.3",
    "ulid-workers": "^2.1.0",
    "vite": "^4.3.9",
    "wouter": "^2.11.0",
    "wrangler": "^3.22.1",
    "yup": "^1.2.0"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/lodash.clamp": "^4.0.7",
    "tailwindcss": "^3.3.3",
    "ulid": "2.3.0"
  }
}
